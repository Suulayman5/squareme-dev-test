{
  "expo": {
    "name": "react-native-task",
    "slug": "react-native-task",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/header.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/image2.png",
        "backgroundColor": "#000000"
      },
      "package": "com.ajah.reactnativetask"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/image2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca3931cb-bd18-4324-938d-27e333ce18cf"
      }
    }
  }
}
